{
    "compilerOptions": {
        "jsx": "react",
        "lib":["ES6","DOM"],
        "target": "ESNext",
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "noEmit": true,
        "skipLibCheck": true
    },
    "exclude": ["./node_modules"]
}
